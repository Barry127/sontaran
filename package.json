{
  "name": "sontaran",
  "version": "0.1.0",
  "description": "Javascript number, string, boolean, object validator",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel index.js --out-dir dist --ignore spec.js --ignore dist/*.js",
    "lint": "clear && eslint .",
    "lint:watch": "onchange \"**/*.js\" -- npm run lint",
    "prepublish": "npm run build",
    "test": "mocha --recursive \"**/*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Barry127/sontaran.git"
  },
  "keywords": [
    "validator",
    "validate",
    "string",
    "number",
    "boolean",
    "object"
  ],
  "author": "Barry de Kleijn <kleijn.barry@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Barry127/sontaran/issues"
  },
  "homepage": "https://github.com/Barry127/sontaran#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "mocha": "^3.2.0",
    "onchange": "^3.2.1",
    "sinon": "^1.17.6"
  }
}
