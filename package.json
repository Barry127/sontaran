  "bugs": {
    "url": "https://github.com/Barry127/sontaran/issues"
  },
  "homepage": "https://barry127.github.io/sontaran",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.17.1",
    "eslint-config-xo-space": "^0.15.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "onchange": "^3.2.1",
    "sinon": "^1.17.6"
  },
  "dependencies": {}
